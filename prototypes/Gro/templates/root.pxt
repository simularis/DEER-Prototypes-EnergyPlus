  end
end

if (not cw_branches.empty?)
  insert "system/cw.pxt", **cw,
    :name => "CW",
    :branch_names => cw_branches,
    :ct_sizing => (1.0/2)
end

if (not wshp_branches.empty?)
  insert "system/sourcewater.pxt",
    :name => "Source Water Plant",
    :branch_names => wshp_branches
end

